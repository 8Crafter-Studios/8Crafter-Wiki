{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "types": ["vite/client", "node"],
    "jsx": "preserve",
    "jsxImportSource": "vue"
  },
  "include": ["docs/main/.vitepress/**/*.ts", "docs/main/.vitepress/**/*.vue"]
}
